    {
      "name": "Aaron Parecki",
      "homepage": "http://aaronparecki.com"
    },
    {
      "name": "Barnaby Walters",
      "homepage": "http://waterpigs.co.uk"
    }
  ],
  "require": {
    "php": ">=5.3.0",
		"mf2/mf2": "~0.2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  "autoload": {
		"files": ["Mf2/functions.php", "Mf2/Shim/Twitter.php", "Mf2/Shim/Instagram.php", "Mf2/Shim/Facebook.php"]
